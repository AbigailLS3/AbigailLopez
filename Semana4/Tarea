    "fig = plt.figure(figsize=(7,7))\n",
    "ax = fig.add_subplot(111)\n",
    "    \n",
    "def init():\n",
    "    ax.set_xlabel(r'$x[m]$',fontsize=15)\n",
    "    ax.set_ylabel(r'$y[m]$',fontsize=15)\n",
    "    ax.set_xlim(-1,1)\n",
    "    ax.set_ylim(-1,1)\n",
    "\n",
    "colors = ['b','k','grey']    \n",
    "sizes = [600, 5, 200]\n",
    "Labels = ['Tierra','Cohete','Luna']\n",
    "\n",
    "def Update(i):\n",
    "    \n",
    "    plot = ax.clear()\n",
    "    init()\n",
    "    plot = ax.set_title(r'$t=%.2f \\ horas$' %(Nt[i]/3600) + r'$ =%.2f \\ días$' %(Nt[i]/86400))        \n",
    "    plot = ax.scatter(0,0, marker='.',color=colors[0],s=sizes[0], label=Labels[0]) #tierra\n",
    "    plot = ax.scatter(Nx[i],Ny[i], marker='.',color=colors[1],s=sizes[1], label=Labels[1])\n",
    "    plot = ax.scatter(Nxl[i],Nyl[i], marker='.',color=colors[2],s=sizes[2], label=Labels[2]) \n",
    "    plot = ax.legend()\n",
    "            \n",
    "    return plot\n",
    "    \n",
    "Animation = anim.FuncAnimation(fig,Update,frames=len(Nt),init_func=init)"
