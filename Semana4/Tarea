    "def Reduce(t,x,y,xl,yl, factor):\n",
    "    Newt = []\n",
    "    Newx = []\n",
    "    Newy = []\n",
    "    Newxl = []\n",
    "    Newyl = []\n",
    "    \n",
    "    for i in range(len(t)):\n",
    "        if i%factor == 0:\n",
    "            Newt.append(t[i])\n",
    "            Newx.append(x[i])\n",
    "            Newy.append(y[i])\n",
    "            Newxl.append(xl[i])\n",
    "            Newyl.append(yl[i])\n",
    "            \n",
    "    return np.array(Newt), np.array(Newx), np.array(Newy), np.array(Newxl), np.array(Newyl)"
   ]
  },
