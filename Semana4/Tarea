   "source": [
    "#Paso a coordenadas cartesianas\n",
    "x = np.zeros(len(Info))\n",
    "y = np.zeros(len(Info))\n",
    "phi = np.zeros(len(Info))\n",
    "\n",
    "\n",
    "for i in range(len(Info)):\n",
    "    x[i] = Info[i][0]*np.cos(Info[i][1])\n",
    "    y[i] = Info[i][0]*np.sin(Info[i][1])\n",
    "    phi[i] = Info[i][1]\n",
    "#Coordenadas de la Luna -Trayectoria circular\n",
    "xl = np.cos(wl*t)\n",
    "yl = np.sin(wl*t)\n",
    "#Coordenadas de la tierra - Inmóvil\n",
    "xt = 0\n",
    "yt = 0\n"
   ]
