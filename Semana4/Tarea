   "outputs": [],
   "source": [
    "#Constantes\n",
    "G  = 6.67*10**-11\n",
    "Mt = 5.9736*10**24\n",
    "Ml = 0.07340*10**24\n",
    "Rt = 6.3781*10**6\n",
    "Rl = 1.7374*10**6\n",
    "d = 3.844*10**8\n",
    "wl = 2.6617*10**(-6)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "metadata": {},
   "outputs": [],
   "source": [
    "#parámetros iniciales\n",
    "ve = 11100.  #Velocidad de escape de la tierra\n",
    "tetha = 5/36.*np.pi\n",
    "phi0 = 0.  \n",
    "r0 = np.array([Rt, Rt])\n",
    "v0 = np.array([ve*np.cos(tetha), ve*np.sin(tetha)])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 55,
   "metadata": {},
   "outputs": [],
   "source": [
    "#para r'~\n",
    "def f1(t, cr, phi, cpr, cpphi):\n",
    "    return cpr"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 56,
   "metadata": {},
   "outputs": [],
   "source": [
    "#para phi'\n",
    "def f2(t, cr, phi, cpr, cpphi):\n",
    "    return cpphi/cr**2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 57,
   "metadata": {},
   "outputs": [],
   "source": [
    "#para derivada de momento lineal\n",
    "def f3(t, cr, phi, cpr, cpphi):\n",
    "    crp = np.sqrt(1 + cr**2 -2*cr*np.cos(phi-wl*t))\n",
    "    return ((cpphi**2)/(cr**3) - (G*Mt/d**3)*(1/(cr**2) + Ml*(cr - np.cos(phi-wl*t))/(Mt*crp**3)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 58,
   "metadata": {},
   "outputs": [],
   "source": [
    "#para derivada de momento angular\n",
    "def f4(t, cr, phi, cpr, cpphi):\n",
    "    crp = np.sqrt(1 + cr**2 -2*cr*np.cos(phi-wl*t))\n",
    "    return -(G*Ml*cr*np.sin(phi-wl*t)/((d*crp)**3))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 59,
   "metadata": {},
   "outputs": [],
   "source": [
    "f = np.array([f1,f2,f3,f4])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 60,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Discretización\n",
    "tmax = 259200*8 #3días*24h*3600s = 259200s, para 8 días\n",
    "tmin = 0\n",
    "h = 10 #pasos de 10s\n",
    "t = np.linspace(tmin, tmax, int(tmax/h))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [],
   "source": [
    "def RGK4(f, R0, t, h):\n",
    "    h = 10  \n",
    "    y = np.zeros([len(t), 4])\n",
    "    y[0] = R0 #condiciones iniciales\n",
    "    K1 = np.zeros([4])\n",
    "    K2 = np.zeros([4])\n",
    "    K3 = np.zeros([4])\n",
    "    K4 = np.zeros([4])\n",
    "    \n",
    "        \n",
    "    for i in tqdm(range(1,len(t))):\n",
    "        for j in range(len(f)):\n",
    "            K1[j] = f[j](t[i-1], y[i-1][0], y[i-1][1], y[i-1][2], y[i-1][3])\n",
    "        for j in range(len(f)):\n",
    "            K2[j] = f[j](t[i-1] + 0.5*h, y[i-1][0] + 0.5*K1[0]*h, y[i-1][1] + 0.5*K1[1]*h, y[i-1][2] + 0.5*K1[2]*h, y[i-1][3] + 0.5*K1[3]*h)\n",
    "        for j in range(len(f)):\n",
    "            K3[j] = f[j](t[i-1] + 0.5*h, y[i-1][0] + 0.5*K2[0]*h, y[i-1][1] + 0.5*K2[1]*h, y[i-1][2] + 0.5*K2[2]*h, y[i-1][3] + 0.5*K2[3]*h)\n",
    "        for j in range(len(f)):\n",
    "            K4[j] = f[j](t[i-1] + h, y[i-1][0] + K3[0]*h, y[i-1][1] + K3[1]*h, y[i-1][2] + K3[2]*h, y[i-1][3] + K3[3]*h)\n",
    "        for j in range(len(f)):\n",
    "            y[i][j] = y[i-1][j] + h*(K1[j] + 2*K2[j] + 2*K3[j] + K4[j])/6\n",
    "\n",
    "    \n",
    "    return y"
   ]
  },
