    "#la c en los nombres de variables indica que se ha usado el cambio de variable. \n",
    "cpr0 = ve*np.cos(tetha-phi0)/d\n",
    "cpphi0 = Rt*ve*np.sin(tetha-phi0)/(d**2)\n",
    "R0 = [Rt/d, phi0, cpr0, cpphi0] #Array de condición inicial\n",
    "print(R0)"
   ]
